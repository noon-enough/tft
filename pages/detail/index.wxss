.detail-main {
    position:absolute;
}

.champion-detail-info {
    display: flex;
    display: -webkit-flex;
    flex-direction: column;
}


.champion-detail-info>view {
    box-sizing: border-box;
    background: var(--background);
}

.champion-detail-info>view:first-child {
    margin-left: 0;
}

.champion-detail-info .detail-info-1 {
    position: relative;
    width: 100%;
    padding-bottom: 30rpx;
}

.champion-detail-info .detail-info-1 .champion-big-pic {
    background-position: top center;
    background-repeat: no-repeat;
    background-size: cover;
    width: 100%;
    height: 380rpx;
    top: 0;
    left: 0;
}

.detail-info-1 .champion-big-pic:before {
    content: "";
    background: url('data:image/png;base64,') no-repeat center;
    background-size: cover;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.detail-info-1 .component-champion {
    position: relative;
    display: block;
    width: 128rpx;
    height: 128rpx;
    line-height: 128rpx;
    border-radius: 50%;
    margin: 40rpx auto 0;
    cursor: default;
    text-align: center;
    z-index: 0;
}

.component-champion .champion-pic {
    position: relative;
    border-radius: 50%;
    width: .68rem;
    height: .68rem;
    box-shadow: #1b212d 0 14px 10px;
    border: 2px solid #c1a25f;
    margin: 0 auto;
}

.detail-info-1 .component-champion .champion-price {
    position: absolute;
    bottom: 20rpx;
    right: 10rpx;
    width: 36rpx;
    height: 36rpx;
    line-height: 36rpx;
    text-align: center;
    font-size: 28rpx;
    border-radius: 50%;
    color: #fff;
    display: block;
}

.component-champion.cost1 .champion-price {
    background-color: #999;
}

.component-champion.cost2 .champion-price {
    background-color: #5fcc29;
}

.component-champion.cost3 .champion-price {
    background-color: #297acc;
}

.component-champion.cost4 .champion-price {
    background-color: #cc29cc;
}

.component-champion.cost5 .champion-price {
    background-color: #cca329;
}

.detail-info-1 .champion-name {
    color: #fff5e0;
    font-size: 32rpx;
    margin: 10rpx 40rpx;
    text-align: center;
    display: flex;
    align-content: center;
    align-items: center;
    justify-content: center;
}

.detail-info-1 .champion-name  .status {
    margin-right: 10rpx;
}

.detail-info-1 .champion-name  .status.plus {
    color: #ff1700;
    font-size: 28rpx;
    font-weight: bold;
}

.detail-info-1 .champion-name  .status.minus {
    color: #3da742;
    font-size: 28rpx;
    font-weight: bold;
}

.champion-skill-list {
    padding: 0 40rpx;
}

.detail-info-1 .champion-skill-list .skill-title {
    margin: 0 0 20rpx;
    color: #fcf0f4;
    font-size: 28rpx;
}

.detail-info-1 .champion-skill-list .champion-skill {
    display: flex;
    display: -webkit-flex;
    align-items: center;
    padding: 0 0 10rpx;
    width: 100%;
}

.skill-pic {
    display: inline-block;
    vertical-align: middle;
    width: 60rpx;
    height: 60rpx;
    border: 1px solid #564c36;
    position: relative;
    flex-shrink: 0;
}

.champion-skill-list .skill-txt {
    display: flex;
    vertical-align: middle;
    margin: 0 10rpx;
    width: 25%;
    flex-direction: column;
}

.detail-info-1 .champion-skill-list .skill-desc {
    display: inline-block;
    vertical-align: middle;
    color: #72788e;
    font-size: 24rpx;
    line-height: 2;
    /*margin-left: 10%;*/
    width: 65%;
}

.champion-skill-list .skill-txt .skill-type {
    color: #7e807d;
    font-size: 24rpx;
}

.champion-skill-list .skill-txt .skill-name {
    color: #ffb60d;
    font-size: 28rpx;
    margin: 0 0 10rpx;
}

.detail-info-2 {
    width: 100%;
    padding-bottom: 180rpx;
}

.detail-info-2 .detail-box {
    padding: 0 40rpx;
    /*border-bottom: 1px solid #192446;*/
    clear: both;
}

.detail-info-title {
    color: #fcf0f4;
    font-size: 28rpx;
    margin: 40rpx 0;
}

.detail-info-title .avatar {
    /*background: #fff9d7;*/
    background: white;
}


.detail-info-title .title {
    color: #fff9d7;
}


.rec-equip-list .rec-equip {
    display: flex;
    justify-content: space-between;
    clear: both;
    margin: 0 0 20rpx;
}


.rec-equip .equip-info {
    float: left;
    display: flex;
}

.rec-equip .equip-formula {
    float: right;
    display: flex;
}

.component-equipment {
    display: inline-block;
    width: 60rpx;
    height: 60rpx;
    border: 1px solid #313537;
    background: #000;
    position: relative;
    cursor: pointer;
    -webkit-transition: border-color,.3s;
    -moz-transition: border-color,.3s;
    -o-transition: border-color,.3s;
    transition: border-color,.3s;
    z-index: 1;
}

.rec-equip .equip-name {
    display: inline-block;
    vertical-align: center;
    color: #ffb60d;
    font-size: 28rpx;
    margin-left: 10rpx;
    line-height: 2;
}

.rec-equip .equip-formula .symbol {
    display: inline-block;
    vertical-align: middle;
    color: #7e807d;
    font-size: 28rpx;
    margin-right: 10rpx;
    font-weight: 700;
    line-height: 2;
}

.rec-equip .equip-formula .symbol-plus {
    margin: 0 10rpx;
}

.detail-info-desc {
    color: #72788e;
    font-size: 28rpx;
    line-height: 2;
}


.detail-box .detail-info-desc .table {
    display: flex;
    flex-direction: column;
    margin-top: 10rpx;
}

.detail-box .detail-info-desc .table .tr {
    display: flex;
    flex-direction: row;
}


.detail-box .detail-info-desc .table .tr .th,
.detail-box .detail-info-desc .table .tr .td {  /*  */
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;            /*  */
    width: calc(100% / 4);
    text-align: center;         /*  */
    justify-content:center;     /*  */
    align-items: center;        /*  */
}

.th {
    padding: 20rpx;
    font-weight: 800;           /*  */
    background-color: var(--background);   /*  */
    color: #fcf0f4;
}

.td {
    padding: 10rpx;
    /*background-color:#edf3e9;   !*  *!*/
    font-size: 26rpx;
}

.champion-detail-info .detail-info-1 .champion-rank-data {
    margin: 20rpx;
}

.champion-detail-info .detail-info-1 .champion-rank-data .champion-data {
    margin: 0 0 20rpx;
    padding: 0 0 20rpx;
    border-bottom: 1px solid #192446;
}

.champion-detail-info .detail-info-1 .champion-rank-data .champion-data, .champion-detail-info .detail-info-1 .champion-rank-data .champion-data .data-item {
}

.champion-detail-info .detail-info-1 .champion-rank-data .champion-data .data-item {
    display: inline-block;
    vertical-align: middle;
    width: 15%;
    margin-right: 4%;
    border-right: 1px solid #192446;
    box-sizing: border-box;
}

.champion-detail-info .detail-info-1 .champion-rank-data .champion-data .data-item .data-num {
    color: #fcf0f4;
    /*font-weight: 700;*/
    height: 50rpx;
}

.champion-detail-info .detail-info-1 .champion-rank-data .champion-data .data-item .data-num.strength {
    color: #fe4b4b;
    font-size: 32rpx;
    font-weight: bolder;
}

.champion-detail-info .detail-info-1 .champion-rank-data .champion-data .data-item:last-child {
    border: none;
    margin-right: 0;
    width: 16%;
}

.champion-detail-info .detail-info-1 .champion-rank-data .champion-data .data-item:first-child {
    margin-left: 40rpx;
}

.champion-detail-info .detail-info-1 .champion-rank-data .champion-data .data-item .data-txt {
    color: #5e647a;
    font-size: 26rpx;
    white-space: nowrap;
}

.champion-detail-info .detail-info-1 .champion-rank-data .champion-data .data-item .data-txt {
    color: #72788e;
}

.capsule-bar-bg {
    width:100%;
    position:absolute;
    top:0;
    left:0;
}

.tft, .lol {
    position: absolute;
    display: inline-block;
    padding: 10rpx 10rpx;
    background-color: rgba(181, 151, 88, 0.5);
    color: #fcf0f4;
    font-size: 28rpx;
    margin: 20rpx 10rpx;
    text-align: right;
    right: 20rpx;
    z-index: 999999;
}
