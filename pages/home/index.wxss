.container {
    background: #002635;
    width: 100%;
    overflow-y:scroll;
    color: #fcf0f4;
}

.capsule-bar-title {
    font-size: unset;
    font-weight: unset;
}

.lineup-item {
    position: relative;
    margin: 0 0 20rpx;
    -webkit-transition: height .3s,border-color .3s;
    -moz-transition: height .3s,border-color .3s;
    -o-transition: height .3s,border-color .3s;
    transition: height .3s,border-color .3s;
    /*background: #2c184e;*/
    background: #024b58;
    border-color: #1f6d7b;
}

.lineup-item .lineup-name {
    display: flex;
    display: -webkit-flex;
    /*color: #fcf0f4;*/
    /*width: 100%;*/
    padding: 20rpx 20rpx 0 20rpx;
    justify-content: space-between;
}

.lineup-item .lineup-name .lineup-quality {
    color: #FF9036;
    font-size: 40rpx;
    font-weight: 700;
    line-height: 50rpx;
    text-align: center;
    margin: 0 0 0 10rpx;
}

.lineup-item .lineup-name .line-name {
    flex: 1;
    font-size: 28rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    font-weight: 600;
}

.lineup-item .lineup-name .line-detail {
    text-align: right;
    font-size: 24rpx;
    color: #a7a3a3;
    font-weight: 500;
}

.lineup-item .lineup-name {
    color: #fcf0f4;
}

.lineup-item .lineup-info {
    /*width: 100%;*/
    position: relative;
    /*background: #283661;*/
    display: flex;
    display: -webkit-flex;
    /*align-items: end;*/
    justify-content: space-between;
    /*height: 120rpx;*/
    padding: 20rpx;
    /*cursor: pointer;*/
    vertical-align: middle;
}


.lineup-item .lineup-info .lineup-champions {
    position: relative;
    width: 100%;
    height: 100%;
    display: flex;
    display: -webkit-flex;
    /*padding-right: 4.6%;*/
    line-height: 180rpx;
    /*border-bottom: 1rpx solid gray;*/
}

