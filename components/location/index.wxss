.lineup-location .icon-carry {
    background: url('data:image/png;base64,') no-repeat center/100%;
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 50%;
    margin-top: -12rpx;
    font-size: smaller;
    pointer-events: none;
    left: -10rpx;
}

.location-champion .icon-carry {
    pointer-events: none;
}

.location-champion .equipment-list {
    position: absolute;
    text-align: center;
    line-height: 1;
    top: 70%;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.component-equipment {
    display: inline-block;
    vertical-align: middle;
    border: 1px solid #313537;
    background: #000;
    position: relative;
    cursor: pointer;
    -webkit-transition: border-color,.3s;
    -moz-transition: border-color,.3s;
    -o-transition: border-color,.3s;
    transition: border-color,.3s;
    z-index: 1;
}

.location-champion .equipment-list .component-equipment {
    border: none;
}
