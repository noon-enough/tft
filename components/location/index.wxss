.lineup-stage-location-wrap {
    margin: 0 20rpx;
    width: 100%;
}

.lineup-location {
    position: relative;
    line-height: 1;
    flex-grow: 0;
    flex-shrink: 0;
    background: url("data:image/png;base64,") no-repeat center/100%;
    width: 600rpx;
    height: 400rpx;
}


.lineup-location .location-champion {
    position: absolute;
    width: 74rpx;
    height: 83rpx;
}


.lineup-location .location-champion .bg-img {
    display: block;
    width: 100%;
    height: 100%;
    background-repeat: no-repeat;
    /*background-size: 140rpx 140rpx;*/
    background-size: cover;
    background-position: center;
    -webkit-mask-image: url(//game.gtimg.cn/images/lol/tft/content-site/slot-mask-img.png);
    mask-image: url(//game.gtimg.cn/images/lol/tft/content-site/slot-mask-img.png);
    -webkit-mask-size: 100%;
    mask-size: 100%;
}



