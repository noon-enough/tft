.component-champion.monster::after{
    content:'';
    position:absolute;
    top:0;
    left:0;
    width: 100rpx;
    height: 100rpx;
    background-repeat:no-repeat;
    background-position:center center;
    background-size:100% 100%;
    pointer-events:none
}
.component-champion.monster.cost2::after{
    background-image:url('data:image/png;base64,')
}
