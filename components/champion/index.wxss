.champion-wrap {
    position: relative;
}

.component-champion {
    position: relative;
    display: inline-block;
    vertical-align: middle;
    cursor: pointer;
    z-index: 1;
}

.component-champion .champion-pic {
    position: relative;
    border-radius: 50%;
    overflow: hidden;
    border: 2px solid #c1a25f;
    margin: 0 auto;
    /*-webkit-transform: scale(1.24);*/
    /*-ms-transform: scale(1.24);*/
    /*-o-transform: scale(1.24);*/
    /*transform: scale(1.24);*/
}

.component-champion .champion-pic {
    position: relative;
    overflow: hidden;
    border: 4rpx solid #c1a25f;
    margin: 0 auto;
}


.component-champion .champion-name {
    color: #a4a8b7;
    font-size: 24rpx;
    text-align: center;
    margin: 30rpx 0 0;
    position: absolute;
    top: 70rpx;
    left: 0;
    width: 100%;
}

.component-champion .champion-stars {
    position: absolute;
    top: -20rpx;
    left: 0;
    width: 100%;
    height: 40rpx;
}

.component-champion .champion-stars > l-avatar {
    position: absolute;
    top: -8rpx;
    left: 35%;
    -webkit-transform: scale(1);
    -ms-transform: scale(1);
    -o-transform: scale(1);
    transform: scale(1);
}

.component-champion .champion-stars.star-3 > l-avatar:nth-of-type(1) {
    top: 10rpx;
    margin-left: -40rpx;
}

.component-champion .champion-stars.star-3 > l-avatar:nth-of-type(2) {
    /*margin-left: -30rpx;*/
}

.component-champion .champion-stars.star-3 > l-avatar:nth-of-type(3) {
    top: 10rpx;
    margin-left: 40rpx;
}

.component-champion .champion-stars .icon {
    background: none;
}

.component-champion .icon-carry {
    background: url('data:image/png;base64,') no-repeat center/100%;
    width: 38rpx;
    height: 38rpx;
    position: absolute;
    top: 50%;
    margin-top: -30rpx;
    left: -15rpx;
    pointer-events: none;
    z-index: 100;
}

.equipment-list {
    position: absolute;
    top: 50%;
    /*left: 12rpx;*/
    margin: 20rpx 0 0;
    width: 100%;
    line-height: 1;
    display: flex;
    align-items: center;
    text-align: center;
    justify-content: center;
}

.component-equipment {
    display: inline-block;
    vertical-align: middle;
    width: 30rpx;
    height: 30rpx;
    border: 1px solid #313537;
    background: #000;
    position: relative;
    cursor: pointer;
    -webkit-transition: border-color,.3s;
    -moz-transition: border-color,.3s;
    -o-transition: border-color,.3s;
    transition: border-color,.3s;
    z-index: 100;
}

.equipment-list > l-avatar {
    display: inline-block;
    position: relative;
}

