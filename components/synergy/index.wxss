.component-synergy {
    position: relative;
    display: inline-block;
    vertical-align: middle;
    cursor: pointer;
}

.component-synergy {
    /*margin: 10rpx 20rpx 10rpx 0;*/
}

.component-synergy .synergy-icon {
    /*line-height: 3.4;*/
    position: relative;
    display: inline-block;
    vertical-align: middle;
    text-align: center;
    overflow: hidden;
    height: 80rpx;
    width: 80rpx;
}

.component-synergy.level4 .synergy-icon,
.component-synergy.level5 .synergy-icon {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: cover;
}
.component-synergy.level3 .synergy-icon {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: cover;
}

.component-synergy.level2 .synergy-icon {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: cover;
}

.component-synergy.level0 .synergy-icon, .component-synergy.level1 .synergy-icon {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: cover;
}

.component-synergy .synergy-icon .icon-img {
    display: inline-block;
    vertical-align: middle;
    width: 40rpx;
    background: none;
    color: white;
    overflow: visible;
}

.component-synergy .synergy-icon .icon-img.rank {
    position: relative;
    top: 10rpx;
}

.component-synergy.active .synergy-icon .icon-img {
    -webkit-filter: contrast(.1) brightness(10);
    filter: contrast(.1) brightness(10);
}

.component-synergy.level4 .synergy-icon .icon-img,
.component-synergy.level5 .synergy-icon .icon-img {
    -webkit-filter: none;
    filter: none;
}

.component-synergy.type-0 .synergy-txt {
    display: inline-block;
    vertical-align: middle;
    background: #091334;
    color: #64625a;
    font-size: 26rpx;
    height: 50rpx;
    line-height: 50rpx;
    padding: 0 20rpx;
    border: 1px solid #272e48;
    margin-left: -10rpx;
}

.component-synergy.type-0.active .synergy-txt {
    color: #fcf0f4;
}

